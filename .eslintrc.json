{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true,
    "mocha": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "chrome": "readonly",
    "documentPictureInPicture": "readonly",
    "parseCommand": "readonly"
  },
  "rules": {
    "no-unused-vars": "off"
  }
}
